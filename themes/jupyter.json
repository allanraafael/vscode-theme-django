{
	"$schema": "vscode://schemas/color-theme",
	"name": "jupyter",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editorLineNumber.foreground": "#ccc",
		"editor.lineHighlightBackground": "#ecebeb",

		"statusBar.foreground": "#24292e",
		"statusBar.background": "#ffffff",
		"statusBar.border": "#ffffff",
		"statusBar.noFolderForeground": "#24292e",
		"statusBar.noFolderBackground": "#fafbfc",
		"statusBar.debuggingForeground": "#24292e",
		"statusBar.debuggingBackground": "#fafbfc",

		"sideBar.foreground": "#000"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#333333ff",
				"background": "#f5f5f5ff"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#408080"
			}
		},
		{
			"name": "docstring Python",
			"scope": [
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"foreground": "#BA2121"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#408080"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation",
				"comment.block.documentation support.class",
				"comment.block.documentation keyword.other.type"
			],
			"settings": {
				"foreground": "#408080"
			}
		},
		{
			"name": "Invalid - Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#96000014"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#96000014",
				"foreground": "#660000"
			}
		},
		{
			"name": "Source",
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator.assignment.",
				"keyword.operator.class.php",
				"keyword.operator.key.php"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"variable.language",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Variables and Inherited class",
			"scope": [
				"variable",
				"support.variable",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Functions Python",
			"scope": [
				"entity.name.function.python",
				"support.function.magic.python",
				"source.css entity.name.function"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"support.class"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"name": "primitive functions and property",
			"scope": [
				"support.function",
				"support.type.python",
				"support.variable.property"
			],
			"settings": {
                "foreground": "#008000"
			}
		},






		{
			"name": "self",
			"scope": [
				"variable.parameter.function.language.special.self.python",
				"variable.language.special.self.python"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "%s",
			"scope": [
				"constant.character.format.placeholder.other.python"
			],
			"settings": {
				"foreground": "#000"
			}
		},

		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constant language",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": [
				"string.regexp",
				"constant.character.set.regexp"
			],
			"settings": {
				"foreground": "#BA2121"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "keyword.operator.quantifier.regexp",
			"settings": {
				"foreground": "#BA2121",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#BA2121"
			}
		},
		{
			"name": "Decorator python",
			"scope": [
				"entity.name.function.decorator",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#AA22FF"
			}
		},
		{
			"name": "type exception",
			"scope": [
				"support.type.exception.python"
			],
			"settings": {
				"foreground": "#000"
			}
		},





		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#666"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#7D9029"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#BC7A00"
			}
		},
		{
			"name": "source html",
			"scope": [
				"text.html"
			],
			"settings": {
				"foreground": "#000"
			}
		},





		{
			"name": "Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css",
				"punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "#BA2121"
			}
		},
		{
			"name": "Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value",
				"source.css constant.numeric",
				"source.css constant.language",
				"source.css keyword.other.unit"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "keyword control css and scss",
			"scope": [
				"source.css keyword.control",
				"source.css punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},





		{
			"name": "Property name json",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#000"
			}
		},





		{
			"name": "this",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "&",
			"scope": [
				"punctuation.definition.variable.php"
			],
			"settings": {
				"foreground": "#0C4B33"
			}
		},
		{
			"name": "blade {{  }}",
			"scope": [
				"support.constant.laravel-blade"
			],
			"settings": {
				"foreground": "#408080"
			}
		},
		{
			"name": "blade directiva @if( )",
			"scope": [
				"entity.name.function.laravel-blade"
			],
			"settings": {
				"foreground": "#BC7A00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "blade text",
			"scope": [
				"source.php.embedded.line.html variable.other.php",
				"source.php.embedded.line.html punctuation.definition.variable.php"
			],
			"settings": {
				"foreground": "#000"
			}
		},





		{
			"name": "Javascript",
			"scope": [
				"keyword.operator.expression.in.js",
				"keyword.operator.expression.typeof.js"
			],
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},




		{
			"name": "Django templates {%   %}      {{  }}",
			"scope": [
				"punctuation.section.tag.twig"
			],
			"settings": {
				"foreground": "#408080"
			}
		},
		{
			"name": "Django templates {%   %}      {{  }}",
			"scope": [
				"keyword.control.twig",
				"keyword.operator.comparison.twig"
			],
			"settings": {
				"foreground": "#BC7A00"
			}
		},
		{
			"name": "Variable Django templates",
			"scope": [
				"variable.other.twig",
				"meta.tag.template.value.twig"
			],
			"settings": {
				"foreground": "#000"
			}
		}
	]
}
