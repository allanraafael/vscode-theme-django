{
	"$schema": "vscode://schemas/color-theme",
	"name": "Django Theme",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#000000",
		"editorLineNumber.foreground": "#ccc",
		"editor.lineHighlightBackground": "#ecebeb",

		"statusBar.foreground": "#24292e",
		"statusBar.background": "#ffffff",
		"statusBar.border": "#ffffff",
		"statusBar.noFolderForeground": "#24292e",
		"statusBar.noFolderBackground": "#fafbfc",
		"statusBar.debuggingForeground": "#24292e",
		"statusBar.debuggingBackground": "#fafbfc",

		"sideBar.foreground": "#0C4B33"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#333333ff",
				"background": "#f5f5f5ff"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring.multi.python",
				"string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
				"string.quoted.docstring.multi.python punctuation.definition.string.end.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#408080"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#408080"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation",
				"comment.block.documentation support.class"
			],
			"settings": {
				"foreground": "#408080"
			}
		},
		{
			"name": "Invalid - Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#96000014"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#96000014",
				"foreground": "#660000"
			}
		},
		{
			"name": "Source",
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#0C4B33"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"support.constant",
				"variable.language",
				"constant.numeric",
				"constant"
			],
			"settings": {
				"foreground": "#19177C"
			}
		},
		{
			"name": "Variables and Inherited class",
			"scope": [
				"variable",
				"support.variable",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#0C4B33"
			}
		},
		{
			"name": "Functions Python",
			"scope": [
				"entity.name.function.python",
				"support.function.magic.python"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0000FF"
			}
		},






		{
			"name": "self",
			"scope": [
				"variable.parameter.function.language.special.self.python",
				"variable.language.special.self.python"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "%s",
			"scope": [
				"constant.character.format.placeholder.other.python"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#BB6688"
			}
		},

		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constant language",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": [
				"string.regexp",
				"constant.character.set.regexp"
			],
			"settings": {
				"foreground": "#BA2121"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "keyword.operator.quantifier.regexp",
			"settings": {
				"foreground": "#BB6688",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#BA2121"
			}
		},
		{
			"name": "Decorator python",
			"scope": [
				"support.type.python",
				"entity.name.function.decorator.python"
			],
			"settings": {
				"foreground": "#AA22FF"
			}
		},
		{
			"name": "type exception",
			"scope": [
				"support.type.exception.python"
			],
			"settings": {
				"foreground": "#D2413A",
				"fontStyle": "bold"
			}
		},





		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#BC7A00"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#0C4B33"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#7D9029"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#BC7A00"
			}
		},
		{
			"name": "source html",
			"scope": [
				"text.html"
			],
			"settings": {
				"foreground": "#0C4B33"
			}
		},





		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css",
				"punctuation.definition.entity.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0C4B33"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#BA2121"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value",
				"source.css keyword"
			],
			"settings": {
				"foreground": "#19177C"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},





		{
			"name": "Django templates {%   %}      {{  }}",
			"scope": [
				"entity.other.django.delimiter.tag",
				"entity.other.django.delimiter.variable"
			],
			"settings": {
				"foreground": "#BC7A00"
			}
		},
		{
			"name": "Variable Django templates",
			"scope": [
				"variable.other.django.block",
				"variable.other.django"
			],
			"settings": {
				"foreground": "#19177C"
			}
		},





		{
			"name": "Property name json",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#0C4B33"
			}
		},





		{
			"name": "this",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "&",
			"scope": [
				"punctuation.definition.variable.php"
			],
			"settings": {
				"foreground": "#0C4B33"
			}
		},
		{
			"name": "blade {{  }}",
			"scope": [
				"support.constant.laravel-blade"
			],
			"settings": {
				"foreground": "#BC7A00"
			}
		},
		{
			"name": "blade directiva @if( )",
			"scope": [
				"entity.name.function.laravel-blade"
			],
			"settings": {
				"foreground": "#BC7A00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "blade text",
			"scope": [
				"source.php.embedded.line.html variable.other.php",
				"source.php.embedded.line.html punctuation.definition.variable.php"
			],
			"settings": {
				"foreground": "#19177C"
			}
		},





		{
			"name": "Javascript",
			"scope": [
				"keyword.operator.expression.in.js",
				"keyword.operator.expression.typeof.js"
			],
			"settings": {
				"foreground": "#008000",
				"fontStyle": "bold"
			}
		},





		{
			"name": "twig keyword control",
			"scope": [
				"keyword.control.twig"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#008000"
			}
		},
		{
			"name": "twig tag {{ }}",
			"scope": [
				"punctuation.section.tag.twig"
			],
			"settings": {
				"foreground": "#BC7A00"
			}
		},
		{
			"name": "twig variable.other.twig",
			"scope": [
				"variable.other.twig",
				"meta.tag.template.value.twig"
			],
			"settings": {
				"foreground": "#19177C"
			}
		},





		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"background": "#FFFFDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"background": "#96000014",
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"background": "#DDDDFF",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#434343"
			}
		}
	]
}
